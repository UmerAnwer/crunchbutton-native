password: m4g4yumm1!
alias: com.crunchbutton

create:
jarsigner -verbose -sigalg SHA1withRSA -digestalg SHA1 \
-keystore cert/android/com.crunchbutton.keystore \
platforms/android/ant-build/CordovaApp-release-unsigned.apk \
com.crunchbutton

sign:
keytool -genkey -v -keystore cert/android/com.crunchbutton.keystore -alias com.crunchbutton -keyalg RSA -keysize 2048 -validity 10000

align:
~/android/sdk/build-tools/19.1.0/zipalign -v 4 platforms/android/ant-build/CordovaApp-release-unsigned.apk platforms/android/ant-build/Crunchbutton.apk